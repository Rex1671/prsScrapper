{
  "name": "prs-scraper-function",
  "version": "1.0.0",
  "description": "Appwrite Cloud Function to scrape PRS India data for MPs and MLAs with AI-powered analysis",
  "main": "src/main.js",
  "type": "module",
  "scripts": {
    "start": "node src/main.js",
    "dev": "node --watch src/main.js",
    "test": "node --test",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.js\"",
    "check": "prettier --check \"src/**/*.js\""
  },
  "keywords": [
    "appwrite",
    "cloud-function",
    "prs-india",
    "scraper",
    "parliament",
    "assembly",
    "mp-tracker",
    "mla-tracker",
    "india-politics",
    "web-scraping",
    "cheerio",
    "political-data",
    "affidavit-data",
    "myneta",
    "ai-analysis"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/prs-scraper-function.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/prs-scraper-function/issues"
  },
  "homepage": "https://github.com/yourusername/prs-scraper-function#readme",
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "node-fetch": "^3.3.2",
    "p-limit": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "^3.2.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 100,
    "arrowParens": "avoid"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2022": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "no-unused-vars": [
        "warn",
        {
          "argsIgnorePattern": "^_"
        }
      ]
    }
  }
}
